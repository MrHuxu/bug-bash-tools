    "cache": "node lib/cache-all",
    "webpack": "./node_modules/webpack/bin/webpack.js --config webpack.config.prd.js",
    "prd-server": "NODE_ENV=production forever start ./server",
    "prd": "npm run cache && npm run webpack && npm run prd-server"
